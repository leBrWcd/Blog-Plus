<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lebrwcd.blog.front.mapper.BlogCategoryMapper">

  <select id="getList" resultType="com.lebrwcd.blog.front.entity.model.BlogCategory">
    select id,name
    from blog_category
    where status = '0' and del_flag != '1'
  </select>
</mapper>
